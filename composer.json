{
    "name": "sabre/resource-locator",
    "description": "The resource-locator is a simple hierachical router-like system.",
    "keywords": [
        "router",
        "resource-locator",
        "tree"
    ],
    "homepage": "http://sabre.io/resource-locator/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.7",
        "sabre/uri" : "~0.0.1"
    },
    "authors": [
        {
            "name": "Evert Pot",
            "email": "me@evertpot.com",
            "homepage": "http://evertpot.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "forum": "https://groups.google.com/group/sabredav-discuss",
        "source": "https://github.com/fruux/sabre-resource-locator"
    },
    "autoload": {
        "psr-4" : {
            "Sabre\\ResourceLocator\\" : "lib/",
            "Sabre\\Uri\\" : "../Uri/lib/"
        },
        "files" : [
            "../Uri/lib/functions.php"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit" : "*"
    },
    "config" : {
        "bin-dir" : "bin/"
    }
}
